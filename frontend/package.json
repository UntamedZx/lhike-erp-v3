    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-simple-maps": "^3.0.6",
    "@types/react-table": "^7.7.20",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "nodemon": "^3.1.9",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
